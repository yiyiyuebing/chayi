import pub.makers.shop.promotion.entity.SaleActivityGood;

import java.io.Serializable;
import java.util.Date;
import java.util.List;

/**
 * Created by devpc on 2017/8/19.
 */
public class SaleActivityGoodVo implements Serializable{
    /**  */
    private String id;

    /** 活动名称 */
    private String name;

    /** 大促标签 */
    private String tag1;

    /** 促销标签 */
    private String tag2;

    /** 大促标签启用 */
    private String tag1Valid;

    /** 促销标签启用 */
    private String tag2Valid;

    /** 促销说明 */
    private String saleDesc;

    /** 活动开始时间 */
    private Date startTime;

    /** 活动结束时间 */
    private Date endTime;

    /** 活动备注 */
    private String memo;

    /** 创建人 */
    private String createBy;

    /** 是否有效 */
    private String isValid;

    /** 删除状态 */
    private String delFlag;

    /** 创建时间 */
    private Date dateCreated;

    /** 更新时间 */
    private Date lastUpdated;

    /**判断是商城还是采购*/
    private String orderBizType;

    /**是否启用单位**/
    private String limitFlag;

    /**限购数量*/
    private Integer limitNum;

    /**限购单位*/
    private String limitUnit;
    //商品集合
    private List<SaleActivityGood> saleActivityGoodList;

    private String goodSkuId;

    private String maxNum;

    private String image;

    public String getGoodSkuId() {
        return goodSkuId;
    }

    public void setGoodSkuId(String goodSkuId) {
        this.goodSkuId = goodSkuId;
    }

    public String getMaxNum() {
        return maxNum;
    }

    public void setMaxNum(String maxNum) {
        this.maxNum = maxNum;
    }

    public String getImage() {
        return image;
    }

    public void setImage(String image) {
        this.image = image;
    }

    public String getId() {
        return id;
    }

    public void setId(String id) {
        this.id = id;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getTag1() {
        return tag1;
    }

    public void setTag1(String tag1) {
        this.tag1 = tag1;
    }

    public String getTag2() {
        return tag2;
    }

    public void setTag2(String tag2) {
