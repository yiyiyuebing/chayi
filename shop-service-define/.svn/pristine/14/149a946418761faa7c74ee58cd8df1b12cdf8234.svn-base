        this.id = saleActivity.getId();
        this.name = saleActivity.getName();
        this.tag1 = saleActivity.getTag1();
        this.tag2 = saleActivity.getTag2();
        this.tag1Valid = saleActivity.getTag1Valid();
        this.tag2Valid = saleActivity.getTag2Valid();
        this.saleDesc = saleActivity.getSaleDesc();
        this.startTime = saleActivity.getStartTime();
        this.endTime = saleActivity.getEndTime();
        this.memo = saleActivity.getMemo();
        this.isValid = saleActivity.getIsValid();
        this.delFlag = saleActivity.getDelFlag();
        this.lastUpdated = saleActivity.getLastUpdated();
        this.dateCreated = saleActivity.getDateCreated();
        this.orderBizType = saleActivity.getOrderBizType();
        this.limitFlag = saleActivity.getLimitFlag();
        this.limitNum = saleActivity.getLimitNum();
        this.limitUnit = saleActivity.getLimitUnit();
