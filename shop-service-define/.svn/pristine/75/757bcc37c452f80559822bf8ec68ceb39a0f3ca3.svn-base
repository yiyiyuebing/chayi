package pub.makers.shop.logistics.vo;

import java.io.Serializable;
import java.math.BigDecimal;

public class FreightTplQueryItem implements Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = -105594070779214163L;
	
	private String goodSkuId;
	private int buyNum;
	private BigDecimal totalAmount;
	private String isSample;
	private String giftFlag;
	
	public String getGoodSkuId() {
		return goodSkuId;
	}
	public void setGoodSkuId(String goodSkuId) {
		this.goodSkuId = goodSkuId;
	}
	public int getBuyNum() {
		return buyNum;
	}
	public void setBuyNum(int buyNum) {
		this.buyNum = buyNum;
	}
	public BigDecimal getTotalAmount() {
		return totalAmount;
	}
	public void setTotalAmount(BigDecimal totalAmount) {
		this.totalAmount = totalAmount;
	}
	public String getIsSample() {
		return isSample;
	}
	public void setIsSample(String isSample) {
		this.isSample = isSample;
	}
	public String getGiftFlag() {
		return giftFlag;
	}
	public void setGiftFlag(String giftFlag) {
		this.giftFlag = giftFlag;
	}
	
}
