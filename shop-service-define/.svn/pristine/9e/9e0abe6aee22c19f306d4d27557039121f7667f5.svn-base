import java.math.BigDecimal;
import java.util.Date;

/**
 * Created by kok on 2017/6/15.
 */
public class PresellPromotionActivityVo extends PromotionActivityVo implements Serializable {
    /**
	 * 
	 */
	private static final long serialVersionUID = -8952009428255869620L;

    private String id;

	/** 预售模式(定金模式/全款模式) */
    private String presellType;

    /** 尾款时间开始 */
