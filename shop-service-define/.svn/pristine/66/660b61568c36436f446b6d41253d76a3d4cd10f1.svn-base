package pub.makers.shop.promotion.vo;

import java.io.Serializable;
import java.util.Date;

/**
 * Created by daiwenfa on 2017/6/19.
 */
public class PresellActivityVo implements Serializable {
    private String id;
    private String name;//活动名称
    private String memo;//活动备注
    private Date presellStart;//预售时间开始
    private Date presellEnd;//预售时间结束
    private Integer num;//状态商品数量
    private String presellType;//预售模式(定金模式/全款模式)
    private Date paymentStart;//尾款时间开始
    private Date paymentEnd;//尾款时间结束
    private Date shipTime;//预计发货时间
    private String image;//产品主图
    private String isValid;//暂停/重启

    public String getId() {
        return id;
    }

    public void setId(String id) {
        this.id = id;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getMemo() {
        return memo;
    }

    public void setMemo(String memo) {
        this.memo = memo;
    }

    public Date getPresellStart() {
        return presellStart;
    }

    public void setPresellStart(Date presellStart) {
        this.presellStart = presellStart;
    }

    public Date getPresellEnd() {
        return presellEnd;
    }

    public void setPresellEnd(Date presellEnd) {
        this.presellEnd = presellEnd;
    }

    public Integer getNum() {
        return num;
    }

    public void setNum(Integer num) {
        this.num = num;
    }

    public String getPresellType() {
        return presellType;
    }

    public void setPresellType(String presellType) {
        this.presellType = presellType;
    }

    public Date getPaymentStart() {
        return paymentStart;
    }

    public void setPaymentStart(Date paymentStart) {
        this.paymentStart = paymentStart;
    }

    public Date getPaymentEnd() {
        return paymentEnd;
    }

    public void setPaymentEnd(Date paymentEnd) {
        this.paymentEnd = paymentEnd;
    }

    public Date getShipTime() {
        return shipTime;
    }

    public void setShipTime(Date shipTime) {
        this.shipTime = shipTime;
    }

    public String getImage() {
        return image;
    }

    public void setImage(String image) {
        this.image = image;
    }

    public String getIsValid() {
        return isValid;
    }

    public void setIsValid(String isValid) {
        this.isValid = isValid;
    }
}
