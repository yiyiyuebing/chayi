    public static TradeGoodVo fromTradeGood(TradeGood good) {
        TradeGoodVo vo = new TradeGoodVo();
        vo.setId(good.getId() == null ? null : good.getId().toString());
        vo.setName(good.getName());
        vo.setSubtitle(good.getSubtitle());
        vo.setSort(good.getSort() == null ? null : good.getSort().toString());
        vo.setType(good.getType());
        vo.setCategory(good.getCategory());
        vo.setPostid(good.getPost());
        vo.setPostFee(good.getPostFee());
        vo.setCreateTime(good.getCreateTime());
        vo.setUpdateTime(good.getUpdateTime());
        vo.setBeginTime(good.getBeginTime());
        vo.setEndTime(good.getEndTime());
        vo.setPostid(good.getPostid() == null ? null : good.getPostid().toString());
        Integer saleNum = good.getSaleNum() == null ? 0 : good.getSaleNum();
        Integer baseSale = good.getBaseSale() == null ? 0 : good.getBaseSale();
        vo.setSaleNum(saleNum);
        vo.setBaseSale(baseSale);
        vo.setTotalSaleNum(saleNum + baseSale);
        vo.setBasePresellNum(good.getBasePresellNum());
        vo.setGroupId(good.getGroupId());
        return vo;
    }
